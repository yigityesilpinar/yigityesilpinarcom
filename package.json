    "generateVersion": "ts-node ./scripts/version",
    "analyze": "npm run cleanup && npm run build:client && webpack-bundle-analyzer dist/loadable-stats.json",
    "build": "run-s 'cleanup' 'build:client' 'build:server'",
    "build:client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig.json\" webpack --config='config/webpack/client.ts'",
    "build:server": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig.json\" webpack --config='config/webpack/server.ts' ",
    "cleanup": "rm -rf ./dist && mkdir -p ./dist",
    "dev": "npm run cleanup && cross-env NODE_ENV=development nodemon",
    "lint": "run-p 'lint:js' 'lint:css'",
    "lint:css": "stylelint './src/**/*.ts'",
    "lint:js": "eslint --ext .tsx,.ts,.js,.jsx .",
    "sort": "npx sort-package-json",
    "production": "cross-env NODE_ENV=production node dist/server.js",
    "test": "jest",
    "type:check": "tsc --noEmit",
    "generateApi": "ts-node scripts/generateApi.ts",
    "get:version": "node -p \"process.env.npm_package_version\""
  },
