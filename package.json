  "main": "index.js",
  "scripts": {
    "analyze": "npm run cleanup && npm run build:client && webpack-bundle-analyzer build/loadable-stats.json",
    "build": "run-s 'cleanup' 'build:client' 'build:server'",
    "build:client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig.json\" webpack -p --config='config/webpack/client.ts'",
    "build:server": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig.json\" webpack -p --config='config/webpack/server.ts' ",
    "cleanup": "rm -rf /build && mkdir -p ./build",
    "dev": "npm run cleanup && cross-env NODE_ENV=development nodemon",
    "generateVersion": "ts-node ./scripts/version",
    "lint": "run-p 'lint:js' 'lint:css'",
    "lint:css": "stylelint './src/**/*.ts'",
    "lint:js": "eslint --ext .tsx,.ts,.js,.jsx .",
    "prod": "cross-env NODE_ENV=production node build/server.js",
    "test": "jest",
    "type:check": "tsc --noEmit"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions"
    ],
    "production": [
      ">5%"
    ]
  },
